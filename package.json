    "css-declaration-sorter": "^4.0.1",
    "cssnano": "^4.1.10",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
